<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
	</configSections>
	<appSettings>
		<add key="ImportFilesLocation" value="SampleFiles"/>
		<add key="ProcessedFilesLocation" value="ProcessedFiles"/>
		<add key="RunAtTheMinuteOfAnHour" value="15"/>
		<!--<add key="RunAtTheMinuteOfAnHour" value="45"/>-->
	</appSettings>
	<connectionStrings>
		<add name="StubLinkColleagueRepository" connectionString="Data Source=.;Initial Catalog=StubLinkColleagueRepository;Integrated Security=True;MultipleActiveResultSets=true" providerName="System.Data.SqlClient" />
	</connectionStrings>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
	</startup>
	<log4net>
		<appender name="RollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
			<file value="Logs\serviceLog.txt" />
			<appendToFile value="true" />
			<rollingStyle value="Size" />
			<maxSizeRollBackups value="10" />
			<maximumFileSize value="10MB" />
			<staticLogFileName value="true" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%-5p %d %5rms %-22.22c{1} %-18.18M - %m%n" />
			</layout>
		</appender>
		<root>
			<appender-ref ref="RollingLogFileAppender" />
		</root>
	</log4net>
</configuration>