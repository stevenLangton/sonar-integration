    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="ida:Tenant" value="SainsburysLink.onmicrosoft.com" />
    <add key="ida:Audience" value="http://sainsburyslink.onmicrosoft.com/linkapi" />
    <add key="ServicesBaseUrl" value="http://localhost:7908/" /><!-- Enterprise Service (or Stub Service for now -->

    <!-- Stub Repository Email domain (fixed for now)- KEEP Active always, less we change in DB -->
    <add key="DbLinkDomain" value="@linktool.onmicrosoft.com" />

    <!--Azure AD email domain specified depending on environment (Keep ONLY ONE Active)-->
    <!--Local Env: linktool.onmicrosoft.com-->
    <!--<add key="AzureLinkDomain" value="@linktool.onmicrosoft.com" />-->

